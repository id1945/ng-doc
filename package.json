    "directory-tree": "^2.2.9",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "glob": "^7.1.7",
    "highlight.js": "^11.0.1",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jstoxml": "^2.1.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
